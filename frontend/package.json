{
  "name": "frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "tsc && prettier --ignore-unknown --check . && eslint --report-unused-disable-directives --max-warnings 0 . && vitest run",
    "fix": "prettier --ignore-unknown --write . && eslint --fix ."
  },
  "dependencies": {
    "react-dom": "^18.3.1"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx,js,cjs,mjs}": [
      "eslint --report-unused-disable-directives --max-warnings 0 --fix",
      "vitest related --passWithNoTests --run"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@tanstack/eslint-plugin-query": "^5.59.1",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.59.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^15.10.0",
    "happy-dom": "^15.7.4",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "typescript": "~5.5.0",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  }
}
